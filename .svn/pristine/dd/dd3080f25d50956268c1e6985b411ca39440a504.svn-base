}
.birdMain{
  display: block;
  width: 5.026042rem;
  height: 1.432292rem;
  background: url("./img/start_0.png") no-repeat center center;
  background-size: cover;
  position: absolute;
  right:0;
  bottom: 0.651042rem;
  transform: translate3d(0,0,0);
  /*animation: mainBird 3s steps(1, start) infinite;*/
  animation: mainBird 3s steps(1, start) infinite;
  animation-fill-mode: both;
}
.logo {
  display: block;
  width:  5.131578rem;
  margin: 0 auto;
  padding-top: 0.263157rem;
  animation: logoScale 2.5s steps(1, start) infinite;
  animation-fill-mode: both;
}
.leaf1{
  display: block;
  background: url("./img/leaf1/leaf1_0.png") no-repeat center center;
  background-size: cover;
  width: 2.4210526rem;
  height: 3rem;
  position: absolute;
  top:0;
  left:-2px;
  animation: leaf1 3s steps(1, start) infinite;
  animation-fill-mode: both;
}
.leaf2{
  display: block;
  background: url("./img/leaf2/leaf2_0.png") no-repeat center center;
  background-size: cover;
  width: 1.6973684rem;
  height: 2.7368421rem;
  position: absolute;
  z-index: 2;
  top:0;
  right:-2px;
  animation: leaf2 3s steps(1, start) infinite;
  animation-fill-mode: both;
}
.cage{
  display: block;
  background: url("./img/cage.png") no-repeat center center;
  background-size: cover;
  width: 0.934210rem;
  height: 2.210526rem;
  position: absolute;
  z-index:1;
  top: 1.315789rem;
  right: 0.394736rem;
  transform-origin: top;
  transform:rotate(0deg);
  animation: cage 3s steps(1, start) infinite;
  animation-fill-mode: both;
}
@keyframes eggAdd {
  10%{
    transform: translate(0,0);
    opacity: 0.2;
  }
  40%{
    transform: translate(-0.131578rem,-0.263157rem);
    opacity: 1;
  }
  80%{
    transform: translate(-0.526315rem,-1.184210rem);
    opacity: 0.6;
  }
  100%{
    transform: translate(-0.789473rem,-1.578943rem);
    opacity: 0;
    display: none;
  }
}
@keyframes eggMinus {
  10%{
    transform: translate(0.921052rem,-1.7105263rem);
    opacity: 0.2;
  }
  40%{
    transform: translate(0.789473rem,-1.578943rem);
    opacity: 1;
  }
  80%{
    transform: translate(0.526315rem,-1.184210rem);
    opacity: 0.6;
  }
  100%{
    transform: translate(0.131578rem,-0.263157rem);
    opacity: 0;
    display: none;
  }
}

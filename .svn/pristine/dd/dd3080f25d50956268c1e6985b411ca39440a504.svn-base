.eggAdd {
  display: block;
  width: 0.585938rem;
  position: absolute;
  right: 0.104167rem;
  bottom: 0.15625rem;
  opacity: 0;
  /*transition:0.5s all cubic-bezier(0.14,-1.33,1,0.18);*/
  -webkit-animation: eggAdd 0.3s ease-in-out;
  animation: eggAdd 0.3s ease-in-out;
}

.eggMinus {
  display: block;
  width: 0.585938rem;
  position: absolute;
  left: 0.104167rem;
  bottom: 0.15625rem;

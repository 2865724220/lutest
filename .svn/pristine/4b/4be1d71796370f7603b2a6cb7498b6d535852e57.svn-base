.result{
    width:100%;
    height:100%;
    background-size: cover;
}
.resultContent{
    left: 50%;
    transform: translateX(-50%);
    position:absolute;
    display: block;
    border:0.05263157894736842rem solid #333;
    width: 6.776315789473684rem;
    height:9.078947368421053rem;
    top: 3.9473684210526314rem;
    border-radius: 0.39473684210526316rem;
    background-color: white;
    opacity: 0.9;
    overflow: hidden;
}
@media (max-width:621px){
    .resultContent{
        left: 50%;
        transform: translateX(-50%);
        position:absolute;
        display: block;
        border:0.05263157894736842rem solid #333;
        width: 6.776315789473684rem;
        height:9rem;
        top: 3.9473684210526314rem;
        border-radius: 0.39473684210526316rem;
        background-color: white;
        opacity: 0.9;
        overflow: hidden;
    }
}
